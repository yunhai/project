{% embed "post/index.twg" %}
    {% block filter %}
        {% embed 'element/master/index/filter.twg' with {'option': option.filter, 'collapse': true} %}
            {% block keyword %}{% endblock %}
            {% block category %}{% endblock %}
        {% endembed %}
    {% endblock %}

    {% block data %}
        {% embed 'store/index/list.twg' %}{% endembed %}
    {% endblock %}
{% endembed %}
