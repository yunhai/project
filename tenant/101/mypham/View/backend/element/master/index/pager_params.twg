{% if data.pager|length > 0 %}
{% set helper = load('url', 'helper') %}
<nav class="col-sm-offset-5 col-sm-4 ">
    <ul class="pagination m-t-none m-b-none">
    {% for item in data.pager  %}
        <li {% if item.target %}class='active'{% endif %}>
            <a href="{{ helper.get({'page': item.page, 'token': option.token }) }}">{{ item.display }}</a>
        </li>
    {% endfor %}
    </ul>
</nav>
{% endif %}